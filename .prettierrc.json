{
  "printWidth": 100,
  "endOfLine": "lf",
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "trailingComma": "all",
  "objectWrap": "collapse",
  "arrowParens": "avoid",
  "singleQuote": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrder": ["^node:", "^@?\\w"]
}
